{
  "name": "ai-bookkeeping-backend",
  "version": "1.0.0",
  "description": "AI 账单汇集工具后端服务",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "setup:prisma": "node scripts/setup-prisma.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "prisma:reset": "npx prisma db push --force-reset && npm run prisma:seed",
    "test": "tap \"test/**/*.test.js\"",
    "test:auth": "tap test/auth.test.js",
    "test:transactions": "tap test/transactions.test.js",
    "test:statistics": "tap test/statistics.test.js"
  },
  "keywords": [
    "fastify",
    "prisma",
    "sqlite",
    "bookkeeping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/static": "^7.0.4",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "fastify": "^5.2.0"
  },
  "devDependencies": {
    "prisma": "^5.22.0",
    "tap": "^21.5.0"
  }
}
